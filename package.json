{
  "name": "skvetchy",
  "version": "2.0.0",
  "homepage": "https://JonathanHarford.github.io/skvetchy",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run package",
    "build:pages": "vite build",
    "package": "svelte-package -o dist",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build",
    "deploy": "npm run build:pages && gh-pages -d dist -f"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.8",
    "@tsconfig/svelte": "^5.0.4",
    "gh-pages": "^6.3.0",
    "jsdom": "^26.1.0",
    "svelte": "^5.28.1",
    "svelte-check": "^4.1.6",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  },
  "dependencies": {
    "konva": "^9.3.20",
    "svelte-konva": "^1.0.0-next.0"
  },
  "peerDependencies": {
    "svelte": "^5.28.1"
  },
  "keywords": [
    "svelte",
    "drawing",
    "canvas",
    "sketching",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JonathanHarford/skvetchy"
  },
  "license": "MIT"
}
